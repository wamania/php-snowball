{
	"name": "wamania/php-snowball",
	"description": "Native PHP Stemmer snowball",
	"keywords": ["stemmer", "porter", "php", "snowball"],
	"license": "MIT",
	"authors": [
		{
			"name": "Wamania",
			"homepage": "http://wamania.com"
		}
	],
	"require": {
		"php": ">=7.3",
		"voku/portable-utf8": "^5.4",
		"symfony/expression-language": "^5.0"
	},
	"require-dev":{
		"symfony/var-dumper": "^4.4",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"psr-4": {
			"Wamania\\Snowball\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Wamania\\Snowball\\Tests\\": "test/"
		}
	}
}
